{
  "name": "idemwallet",
  "version": "1.0.0",
  "description": "Ledger-based transfer backend in Node.js, Express, Sequelize with TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "test": "jest --coverage"
  },
  "dependencies": {
    "big.js": "^7.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "@types/express": "^4.17.20",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.5.1",
    "@types/uuid": "^9.0.2",
    "jest": "^30.2.0",
    "sequelize-cli": "^6.6.5",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
